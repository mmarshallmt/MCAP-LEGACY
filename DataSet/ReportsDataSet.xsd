and (Vehicle.BreakDt BETWEEN  @CreateDtFrom AND @CreateDtTo)
and DateDiff(day,Vehicle.BreakDt,GetDate())&lt;=90
and (Envelope.EnvelopeId=@envelopeid or @envelopeid=0)
