                         CASE WHEN V.FlashInd = 1 AND V.NationalInd = 1 THEN 'National' WHEN V.FlashInd = 1 THEN 'Flash' ELSE 'Regular' END AS FlashStatus, 
                         S.Name AS Sender, V.Priority, V.CreateDt, C.Descrip AS Status, U.FName + ' ' + U.LName AS ReQCedBy, V2.ReQCDt AS ReQCedDate, 
                         CASE WHEN V.ActualPageCount = 0 THEN (Select COUNT(*) from VehiclePages Where VehicleId = V.VehicleId) ELSE V.ActualPageCount END As PageCount
, CASE WHEN (Select COUNT(*) from VehiclePages Where VehicleId = V.VehicleId) &gt; 0 THEN 'Exported' ELSE '' END As ExportStatus, 'Details' as Details
,(select top 1  c.descrip from RetMktCoverage rmc join code c on rmc.coverageid = c.codeid where rmc.mktid=v.MktId and rmc.retid=v.RetId) Coverage
,
 V.CircularId,
 (select min(Flyerid) from vehiclePages with(nolock) where vehicleid= v.VehicleId) as Flyerid , L.Descrip as [Language]
FROM            vwCircular AS V INNER JOIN
                         Vehicle as V2  with(nolock) ON v.VehicleId = V2.VehicleId LEFT OUTER JOIN
                         Ret AS R ON V.RetId = R.RetId AND V.VehicleId = @VehicleId INNER JOIN
                         Mkt AS M ON V.MktId = M.MktId INNER JOIN
                         Media AS Med ON V.MediaId = Med.MediaID LEFT OUTER JOIN
                         Publication AS P ON V.PublicationId = P.PublicationId  LEFT OUTER JOIN
                         [User] AS U ON V2.ReQCedById = U.UserID LEFT OUTER JOIN
                         Code AS C ON V.StatusID = C.CodeId 
						   Left  OUTER JOIN Envelope as E on V.EnvelopeId=E.EnvelopeId
					  Left  OUTER JOIN Sender as S on S.SenderId=E.SenderId 
					  left join [Language] as L on L.languageid = v.Languageid
ORDER BY V.VehicleId</CommandText>
