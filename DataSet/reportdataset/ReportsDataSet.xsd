		, V.EventId, E.Descrip [Event], V.ThemeId, T.Descrip [Theme]
		, CASE WHEN FlashInd = 1 THEN 'Yes' ELSE 'No' END [Flash]
		, CASE WHEN NationalInd = 1 THEN 'Yes' ELSE 'No' END [National]
		, CreateDt [Created On], CreatedById, C.FName + ' ' + C.LName [Created By]
		, IndexDt [Indexed On], IndexedById, I.FName + ' ' + I.LName [Indexed By]
		, ScanDt [Scanned On], ScannedById, S.FName + ' ' + S.LName [Scanned By]
		, CreateSizedDt [Sized On], V.StatusId, VS.Descrip
FROM vwCircular V
	INNER JOIN Media Med ON V.MediaId = Med.MediaId 
	INNER JOIN Mkt M ON V.MktId =  M.MktId
	INNER JOIN Publication P ON V.PublicationId = P.PublicationId
	INNER JOIN Ret R ON V.RetId = R.RetId
	LEFT JOIN [Language] L ON V.LanguageId = L.LanguageId
	LEFT JOIN [Event] E ON V.EventId = E.EventId
	LEFT JOIN Theme T ON V.ThemeId = T.ThemeId
	INNER JOIN [User] C ON V.CreatedById = C.UserId
	LEFT JOIN [User] I ON V.IndexedById = I.UserId
	LEFT JOIN [User] S ON V.ScannedById = S.UserId
	LEFT JOIN [User] Q ON V.QCedById = Q.UserId
	INNER JOIN vwVehicleStatus VS ON V.StatusId = VS.CodeId AND VS.Descrip IN ('Review', 'Duplicate')
where ( r.RetId = @RetId OR @RetId = 0)
and ( V.VehicleId = @VehicleId OR @VehicleId = 0 )
and (VS.Descrip = @Descrip OR @Descrip = 'all')
and (CreateDt BETWEEN @StartDt AND @EndDt OR (@Descrip = 'all' OR @Descrip = 'Review'))</CommandText>
