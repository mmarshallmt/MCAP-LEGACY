      <Query>
        <DataSourceName>MCAPConnectionString</DataSourceName>
        <CommandText>SELECT     CTLs.CCTaskLogsId, CTLs.Assigned, CTLs.Date, CTLs.Time, CTLs.DueDate, CTLs.Status, CTLs.Urgency, CTLs.ActionType, CTLs.SpecificAction, 
                      S.Name AS 'Sender', CTLs.MissingAd, CTLs.MissingIdValue, R.Descrip AS 'Retailer', M.Descrip AS 'Market', Md.Descrip AS 'Media', CTLs.AdDate, 
                      P.Descrip AS 'Publication', CTLs.OtherContactName, CTLs.PhoneNoCalled, CTLs.PersonSpokeWith,  CTLs.Comments, 
                      CTLs.Reminders, U.FName + ' ' + U.LName AS 'UserId'
FROM         CCTaskLogs AS CTLs INNER JOIN
                      Media AS Md ON Md.MediaID = CTLs.MediaId INNER JOIN
                      Mkt AS M ON M.MktId = CTLs.MarketId INNER JOIN
                      Ret AS R ON R.RetId = CTLs.RetailerId INNER JOIN
                      Sender AS S ON S.SenderId = CTLs.SenderId INNER JOIN
                      Publication AS P ON P.PublicationId = CTLs.PublicationId INNER JOIN
                      [User] AS U ON U.UserID = CTLs.UserId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
